label live-amd64
  menu label ^NetLinux Desktop (Live)
  menu default
  linux /live/vmlinuz
  initrd /live/initrd.img
  append boot=live components quiet splash

label live-amd64-safe
  menu label NetLinux Desktop (^safe graphics)
  linux /live/vmlinuz
  initrd /live/initrd.img
  append boot=live components quiet splash nomodeset

label install
  menu label ^Install NetLinux Desktop
  linux /live/vmlinuz
  initrd /live/initrd.img
  append boot=live components quiet splash install-netlinux

label live-amd64-failsafe
  menu label NetLinux Desktop (^failsafe)
  linux /live/vmlinuz
  initrd /live/initrd.img
  append boot=live components memtest noapic noapm nodma nomce nolapic nomodeset nosmp nosplash vga=normal
